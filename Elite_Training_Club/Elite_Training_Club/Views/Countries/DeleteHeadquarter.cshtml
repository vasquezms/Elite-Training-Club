@model Elite_Training_Club.Data.Entities.Headquarter

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>¿Está seguro de querer borrar?</h3>
<div>

    <partial name="_HeadquarterDetails" />
    
    <form asp-action="DeleteHeadquarter">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-outline-danger" /> |
        <a asp-action="DetailsCity" asp-route-id="@Model?.City.Id" class="btn btn-outline-success" >Regresar</a>
    </form>
</div> 
